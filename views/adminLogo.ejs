<%- include('partials/adminHeader')%>
   
    
    <div class="sidebar">
        <div class="frontend-settings">
          <p>Manage Logo</p>
        </div>
    

    <main class="main-content">
        <div class="content">
            <h2 style="font-size: 25px; margin-bottom: 10px;">Upload Logo</h2>
            <% if (data.logo_path) { %>
                <p>Current Logo:</p>
                <img src="<%= data.logo_path %>" alt="Current Logo" style="max-width: 200px;">
            <% } %>
            <form style="display: flex; flex-direction: column; " action="/backend/logo" method="POST" enctype="multipart/form-data">
                <label for="logo" style="margin-bottom: 10px;">Select Logo Image:</label>
                <input type="file" name="logo" id="logo" accept="image/*" required>
                <br><br>
                <button type="submit" class=" btn btn-primary">Upload Logo</button>
            </form>
            <br>
            <a style="margin-top: 20px;" href="/backend/dashboard">Back to Dashboard</a>
        </div>
    </main>
</div>
<script src="/js/admin.js"></script>
<script src="/js/adminstyle.js"></script>
</body>
</html>